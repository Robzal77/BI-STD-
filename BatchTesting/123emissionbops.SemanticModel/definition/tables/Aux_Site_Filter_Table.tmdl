	partition Aux_Site_Filter_Table = calculated
		mode: import
		source = ```
				
				VAR SitesBase =
				    FILTER (
				        SUMMARIZE (
				            dim_org_greenbridge_hierarchy_master,
				            dim_org_greenbridge_hierarchy_master[Site_Code],
				            "Site_Name", MAX ( dim_org_greenbridge_hierarchy_master[Site_Name] ),
				            "Country",   MAX ( dim_org_greenbridge_hierarchy_master[Country] ),
				            "Zone",      MAX ( dim_org_greenbridge_hierarchy_master[Zone] ),
				            "PlantType", MAX ( dim_org_greenbridge_hierarchy_master[Plant_Type] ),
				            "Value",     [Change 2024 2025]
				        ),
				        [PlantType] = "BOP"
				    )
				
				
				VAR SitesWithDelta =
				    ADDCOLUMNS (
				        SitesBase,
				        "AxisType",  "Site",
				        "AxisLabel", [Site_Name],
				        "Site_ID",   [Site_Code],
				        "DeltaValueSite",
				            VAR curSite = [Site_Code]
				            RETURN
				                CALCULATE (
				                    [Change 2024 2025],
				                    TREATAS ( ROW ( "Site_Code", curSite ), dim_org_greenbridge_hierarchy_master[Site_Code] )
				                )
				    )
				
				
				VAR SiteRankTable =
				    SELECTCOLUMNS ( SitesWithDelta, "Site_ID", [Site_ID], "SiteMetric", [DeltaValueSite] )
				
				VAR SiteRanks =
				    ADDCOLUMNS (
				        SiteRankTable,
				        "SiteRank", RANKX ( SiteRankTable, [SiteMetric], [SiteMetric], ASC, DENSE )
				    )
				
				VAR SiteRanksSlim =
				    SELECTCOLUMNS ( SiteRanks, "Site_ID", [Site_ID], "SiteRank", [SiteRank] )
				
				
				VAR ZoneList =
				    DISTINCT ( SELECTCOLUMNS ( SitesWithDelta, "Zone", [Zone] ) )
				
				VAR ZonesWithMetric =
				    ADDCOLUMNS (
				        ZoneList,
				        "ZoneMetric",
				            CALCULATE (
				                [Change 2024 2025],
				                TREATAS ( ROW ( "Zone", [Zone] ), dim_org_greenbridge_hierarchy_master[Zone] )
				            )
				    )
				
				VAR ZonesRanks =
				    ADDCOLUMNS (
				        ZonesWithMetric,
				        "ZoneRank", RANKX ( ZonesWithMetric, [ZoneMetric], [ZoneMetric], ASC, DENSE )
				    )
				
				VAR ZonesRanksSlim =
				    SELECTCOLUMNS ( ZonesRanks, "Zone", [Zone], "ZoneRank", [ZoneRank] )
				
				
				VAR CountryList =
				    DISTINCT ( SELECTCOLUMNS ( SitesWithDelta, "Country", [Country] ) )
				
				VAR CountriesWithMetric =
				    ADDCOLUMNS (
				        CountryList,
				        "CountryMetric",
				            CALCULATE (
				                [Change 2024 2025],
				                TREATAS ( ROW ( "Country", [Country] ), dim_org_greenbridge_hierarchy_master[Country] )
				            )
				    )
				
				VAR CountriesRanks =
				    ADDCOLUMNS (
				        CountriesWithMetric,
				        "CountryRank", RANKX ( CountriesWithMetric, [CountryMetric], [CountryMetric], ASC, DENSE )
				    )
				
				VAR CountriesRanksSlim =
				    SELECTCOLUMNS ( CountriesRanks, "Country", [Country], "CountryRank", [CountryRank] )
				
				
				VAR WithSiteRank =
				    NATURALLEFTOUTERJOIN ( SitesWithDelta, SiteRanksSlim )
				
				VAR WithZoneRank =
				    NATURALLEFTOUTERJOIN ( WithSiteRank, ZonesRanksSlim )
				
				VAR FullRanked =
				    NATURALLEFTOUTERJOIN ( WithZoneRank, CountriesRanksSlim )
				
				
				VAR FinalRows =
				    ADDCOLUMNS ( FullRanked, "AxisSortSite", [SiteRank] + 1000000, "AxisOrder", 1 )
				
				RETURN
				UNION (
				    ROW (
				        "AxisType",     "Start",
				        "AxisLabel",    "Period 1",
				        "AxisOrder",    -1,
				        "Site_ID",      BLANK (),
				        "AxisSortSite", 0,
				        "SiteRank",     0,
				        "ZoneRank",     -1,
				        "CountryRank",  -1,
				        "Zone",         "Period 1",
				        "Country",      "Period 1",
				        "SiteName",     "Period 1",
				        "PlantType",    "⟂ Totals",
				        "Value",        [Emissions Period 1]
				    ),
				
				    SELECTCOLUMNS (
				        FinalRows,
				        "AxisType",     [AxisType],
				        "AxisLabel",    [AxisLabel],
				        "AxisOrder",    [AxisOrder],
				        "Site_ID",      [Site_ID],
				        "AxisSortSite", [AxisSortSite],
				        "SiteRank",     [SiteRank],
				        "ZoneRank",     [ZoneRank],
				        "CountryRank",  [CountryRank],
				        "Zone",         [Zone],
				        "Country",      [Country],
				        "SiteName",     [Site_Name],
				        "PlantType",    [PlantType],
				        "Value",        [Value]
				    ),
				
				    ROW (
				        "AxisType",     "End",
				        "AxisLabel",    "Period 2",
				        "AxisOrder",    9999999999,
				        "Site_ID",      BLANK (),
				        "AxisSortSite", 2000000000,
				        "SiteRank",     2000000000,
				        "ZoneRank",     2000000000,
				        "CountryRank",  2000000000,
				        "Zone",         "Period 2",
				        "Country",      "Period 2",
				        "SiteName",     "Period 2",
				        "PlantType",    "⟂ Totals",
				        "Value",        [Emissions Period 2]
				    )
				)
				
				```

