		sourceColumn: Date Time

		variation Variation
			isDefault
			relationship: d53781b5-8e0c-4f3a-8aae-535b9b36dad2
			defaultHierarchy: LocalDateTable_ef2b808d-2eee-47e0-a352-9fcff0dfc65a.'Date Hierarchy'

		annotation SummarizationSetBy = Automatic

	partition DataLastRefreshed = m
		mode: import
		source =
				let
