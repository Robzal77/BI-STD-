table DataLastRefreshed
	lineageTag: d30a2236-3f55-4dfa-b6e6-4d2114600cb5

	measure 'Data Refresh' =
			
			"Last Power BI Refresh Date: "
			& FORMAT(
			    FIRSTDATE('DataLastRefreshed'[Date Time]),
			    "mmm-dd-yyyy hh:mm"
			)
			& " (UTC)"
		lineageTag: d8b4225a-3674-47c4-aacb-b736ee06e613

	measure 'Data Refresh BM' =
			
			"Last BM Refresh Date: "
			    & FORMAT ( TODAY(), "MMM-DD-YYYY")
		lineageTag: ed621cf7-af4a-4ccb-9610-b964a743a90d

	column 'Date Time'
		dataType: dateTime
		formatString: General Date
		lineageTag: 569bdb06-b953-4ad4-b7c6-10dc6bc2ed80
