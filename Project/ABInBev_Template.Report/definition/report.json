{
    "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/report/3.1.0/schema.json",
    "themeCollection": {
        "baseTheme": {
            "name": "CY20SU09",
            "version": "1.0",
            "type": "BuiltIn"
        },
        "customTheme": {
            "name": "ABInBev_Default_Report",
            "type": "SharedResources"
        }
    },
    "objects": {
        "section": [
            {
                "properties": {
                    "verticalAlignment": {
                        "expr": {
                            "Literal": {
                                "Value": "'Top'"
                            }
                        }
                    }
                }
            }
        ],
        "outspacePane": [
            {
                "properties": {
                    "expanded": {
                        "expr": {
                            "Literal": {
                                "Value": "false"
                            }
                        }
                    }
                }
            }
        ]
    },
    "resourcePackages": [
        {
            "name": "SharedResources",
            "type": "SharedResources",
            "items": [
                {
                    "name": "ABInBev_Default_Report",
                    "path": "BaseThemes/ABInBev_Default_Report.json",
                    "type": "BaseTheme"
                }
            ]
        },
        {
            "name": "RegisteredResources",
            "type": "RegisteredResources",
            "items": [
                {
                    "name": "Default_Report_Page.png",
                    "path": "Default_Report_Page.png",
                    "type": "Image"
                }
            ]
        }
    ],
    "settings": {
        "useStylableVisualContainerHeader": true,
        "exportDataMode": "AllowSummarized",
        "defaultDrillFilterOtherVisuals": true,
        "allowChangeFilterTypes": true,
        "useEnhancedTooltips": false
    }
}